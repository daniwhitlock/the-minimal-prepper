                                  <option value="3">Three</option>
                                  <option value="4">Four</option>
                                  <option value="1">Five</option>
                                  <option value="1">Six</option>
                                  <option value="1">Seven</option>
                                </select>
                                <label for="floatingSelect">Select the number of those who are older than the age of 7.</label>
                            </div>
                           
                            <!--time-->
                            <div class="form-floating mb-3">
                                <select class="form-select" id="floatingSelect" aria-label="Floating label select example">
                                  <option selected>2 weeks</option>
                                  <option value="1">1 month</option>
                                  <option value="2">3 months</option>
                                  <option value="3">6 months</option>
                                  <option value="1">9 months</option>
                                </select>
                                <label for="floatingSelect">How much time are you wanting to prepare for?</label>
                            </div>

                            <!--Dietary options-->
                            <div class="form-floating mb-3">
